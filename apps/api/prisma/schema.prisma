datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Property {
  id          Int      @id @default(autoincrement())
  title       String
  price       Float
  location    String
  description String
  image       String
  createdAt   DateTime @default(now())
}
